{
	"parser": "babel-eslint",
	"extends": [
		"flowtype",
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [ "plugin:flowtype/recommended" ]
	,
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"prettier/prettier": "error"
}